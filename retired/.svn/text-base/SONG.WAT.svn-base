create table song (
	cd_nbr      smallint    not null,
	song_nbr    smallint    not null,
	song_nm     varchar(60) not null,
	song_len    char(5)     not null,
        artist_nbr  smallint,
        FOREIGN KEY cd_nbr      REFERENCES cd,
	FOREIGN KEY artist_nbr  REFERENCES artist,
        PRIMARY KEY (cd_nbr, song_nbr),
        CHECK (cd_nbr > 0 AND song_nbr > 0 AND song_len LIKE '__:__'));
create index song_1 on song (song_nm);
create index song_2 on song (song_len);
