$'echo Dropping table CD...';
drop table cd;
$'echo Creating table CD...';
create table cd (
	cd_nbr     integer  not null,
	cd_desc    char(55) not null,
	artist_nbr integer  not null,
	label_cd   char(3)  not null,
	label_id   char(15) not null,
	song_cnt   integer  not null,
	cd_tm      char(5)  not null,
    code_1     char(3));
$'echo Creating indices for table CD...';
create unique index cd_0 on cd (cd_nbr);
create index cd_1 on cd (artist_nbr);
create index cd_2 on cd (label_cd);
create index cd_3 on cd (cd_desc);
$'echo Loading data for table CD...';
run cd.dat;
